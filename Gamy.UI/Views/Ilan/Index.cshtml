                @foreach (var item in Model.SubCategory.Products)
                {
                    <li class="col-xl-2 col-lg-3 col-md-4 col-sm-6 col-12 mb-3 mobil-boyut">
                        <div class="product">
                            <div class="p-1">
                                <div class="product-img">
                                    <figure>
                                        <a href="/product/@item.Name/@item.Id">
                                            <img class="lazy" data-original="/GamyEpin/uploads/advert/minimum/44237592027245-kapak-35657470103571.jpg" alt="" src="https://www.epinkasa.com/uploads/advert/minimum/44237592027245-kapak-35657470103571.jpg" style="">
                                        </a>
                                    </figure>

                                    <div class="featured" @(item.OneCikanUrun == true ? "" : "hidden")> Öne Çıkan</div>
                                    <div class="showcase" @(item.Vitrin == true ? "" : "hidden")>Vitrin İlanı</div>
                                    <div class="user-wrapper">
                                        <div class="user-profile">
                                            <a href="../../profile/Reyo.html"><img class="lazy" src="../../images/avatar/001-gamer.png" alt="" data-toggle="tooltip" data-placement="top" title="" data-original-title="Reyo"></a>
                                            <div class="user-infos">
                                                <a href="../../profile/Reyo.html">@item.User.UserName</a>
                                                <span>Satıcı</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="product-infos">
                                    <a href="../random-hesap/4464.html" class="text-truncate">@item.Name</a>
                                    <span>@item.Description</span>
                                </div>
                                <div class="product-price">
                                    <div class="price">@item.Price</div>
                                    <a href="/product/@item.Name/@item.Id" class="go"><i class="icon-right-open"></i></a>
                                </div>
                            </div>
                        </div>
                    </li>
                }
            </ul>
        </div>
        <div class="mt-xl-4 mt-3 bg-white">
            <div class="ilan-desc-title">
                @Model.Name
            </div>
            <div class="p-3">
                <div class="ilan-desc-text fs-12">
                    @Model.Description
                </div>
            </div>
        </div>
    </div>
</main>